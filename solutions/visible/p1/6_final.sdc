# SDC Constraints generated by ASU Tapeout Agent
# Clock constraint
create_clock -name clk -period 1.1 [get_ports clk]

# Input delays (20% of clock period)
set_input_delay -clock clk -max 0.22000000000000003 [get_ports {data_in}]
set_input_delay -clock clk -min 0 [get_ports {data_in}]

# Output delays (20% of clock period)  
set_output_delay -clock clk -max 0.22000000000000003 [get_ports {detected}]
set_output_delay -clock clk -min 0 [get_ports {detected}]

# Clock uncertainty (5% of clock period)
set_clock_uncertainty 0.05500000000000001 [get_clocks clk]

# Don't touch the clock network
set_dont_touch_network [get_clocks clk]
